        { id: 'demographics', name: 'Р”РµРјРѕРіСЂР°С„РёС‡РµСЃРєРёРµ РїРѕРєР°Р·Р°С‚РµР»Рё РїРѕ СЂРµРіРёРѕРЅР°Рј' },
        { id: 'funding', name: 'Р¤РёРЅР°РЅСЃРёСЂРѕРІР°РЅРёРµ Р·РґСЂР°РІРѕРѕС…СЂР°РЅРµРЅРёСЏ' },
        { id: 'satisfaction', name: 'РЈРґРѕРІР»РµС‚РІРѕСЂРµРЅРЅРѕСЃС‚СЊ РјРµРґРёС†РёРЅСЃРєРёРјРё СѓСЃР»СѓРіР°РјРё' },
        { id: 'doctors', name: 'Р Р°СЃРїСЂРµРґРµР»РµРЅРёРµ РјРµРґРёС†РёРЅСЃРєРёС… РєР°РґСЂРѕРІ' },
